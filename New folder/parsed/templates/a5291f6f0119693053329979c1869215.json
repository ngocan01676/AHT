{"components/common/custom-subscription-form":"<h3>Want to be notified?</h3>\r\n<form class=\"form\" id=\"form-newletter\" action=\"{{urls.subscribe.action}}\" method=\"post\">\r\n    <fieldset class=\"form-fieldset\">\r\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\r\n         <div class=\"form-field\">\r\n            <label class=\"form-label\" for=\"nl_first_name\">Name *</label>\r\n        <input type=\"text\" name=\"nl_first_name\" value=\"\" required=\"\">\r\n        </div>\r\n        <input type=\"hidden\" name=\"check\" value=\"1\">\r\n        <div class=\"form-field\">\r\n            <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\r\n            <div class=\"form-prefixPostfix wrap\">\r\n                <label class=\"form-label\" for=\"nl_email\">Email Address *</label>\r\n                <input class=\"form-input\" id=\"nl_email\" name=\"nl_email\" type=\"email\" value=\"{{customer.email}}\" placeholder=\"\" required>\r\n                <input class=\"button button-primary form-prefixPostfix-button--postfix\" type=\"submit\" value=\"Notify me\">\r\n            </div>\r\n            {{#if settings.show_newsletter_summary }}\r\n                <div class=\"footer-newsletter-summary\">{{settings.newsletter_summary}}</div>\r\n            {{/if}}\r\n        </div>\r\n    </fieldset>\r\n</form>\r\n"}
