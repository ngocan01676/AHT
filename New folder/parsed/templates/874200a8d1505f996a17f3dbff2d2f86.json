{"components/cart/coupon-input":"<div class=\"cart-total-value\">\r\n\r\n    <button class=\"coupon-code-add\">{{lang 'cart.coupons.add_coupon'}}</button>\r\n\r\n    <button class=\"coupon-code-cancel\" style=\"display: none;\">{{lang 'cart.coupons.cancel'}}</button>\r\n</div>\r\n\r\n<div class=\"cart-form coupon-code\" style=\"display: none;\">\r\n    <form class=\"form form--hiddenLabels coupon-form\" method=\"post\" action=\"{{urls.cart}}\">\r\n        <label class=\"form-label\" for=\"couponcode\">{{lang 'cart.coupons.coupon_code'}}</label>\r\n        <input class=\"form-input\" data-error=\"{{lang 'cart.coupons.empty_error'}}\"  id=\"couponcode\" type=\"text\" name=\"couponcode\" value=\"\" placeholder=\"{{lang 'cart.coupons.coupon_code'}}\">\r\n        <input class=\"button button--primary button--small\" type=\"submit\" value=\"{{lang 'cart.coupons.button'}}\">\r\n        <input type=\"hidden\" name=\"action\" value=\"applycoupon\">\r\n    </form>\r\n</div>\r\n","components/cart/gift-certificate-input":"<div class=\"cart-total-value\">\r\n\r\n    <button class=\"gift-certificate-add\">{{lang 'cart.gift_certificates.gift_certificate'}}</button>\r\n\r\n    <button class=\"gift-certificate-cancel\" style=\"display: none;\">{{lang 'cart.coupons.cancel'}}</button>\r\n</div>\r\n\r\n<div class=\"cart-form gift-certificate-code\" style=\"display: none;\">\r\n    <form class=\"form form--hiddenLabels cart-gift-certificate-form\" method=\"post\" action=\"{{urls.cart}}\">\r\n        <label class=\"form-label\" for=\"certcode\">{{lang 'cart.gift_certificates.cert_code'}}</label>\r\n        <input class=\"form-input\" data-error=\"{{lang 'cart.gift_certificates.empty_error'}}\"  id=\"certcode\" type=\"text\" name=\"certcode\" value=\"\" placeholder=\"{{lang 'cart.gift_certificates.add_cert_code'}}\">\r\n        <input class=\"button button--primary button--small\" type=\"submit\" value=\"{{lang 'cart.coupons.button'}}\">\r\n        <input type=\"hidden\" name=\"action\" value=\"applycoupon\">\r\n    </form>\r\n</div>\r\n","components/cart/totals":"<ul class=\"cart-totals\">\r\n        {{#if cart.discount }}\r\n            <li class=\"cart-total\">\r\n                <div class=\"wp-content clearfix\">\r\n                    <div class=\"cart-total-label\">\r\n                        <strong>{{lang 'cart.discount'}}:</strong>\r\n                    </div>\r\n                    <div class=\"cart-total-value\">\r\n                        {{cart.discount.formatted}}\r\n                    </div>\r\n                </div>\r\n            </li>\r\n        {{/if}}\r\n        {{#if cart.coupons}}\r\n        {{#each cart.coupons}}\r\n            <li class=\"cart-total\">\r\n                <div class=\"wp-content clearfix\">\r\n                    <div class=\"cart-total-label\">\r\n                        <strong>{{lang 'cart.coupons.code_label' code=code}}</strong>\r\n                        <div>\r\n                            <a href=\"{{remove_url}}\">{{lang 'cart.coupons.remove'}}</a>\r\n                        </div>\r\n                    </div>\r\n                    <div class=\"cart-total-value\">\r\n                        <span>{{discount.formatted}}</span>\r\n                    </div>\r\n                </div>\r\n            </li>\r\n        {{/each}}\r\n    {{else}}\r\n        <li class=\"cart-total\">\r\n            <div class=\"wp-content clearfix\">\r\n                <div class=\"cart-total-label\">\r\n                    <strong>{{lang 'cart.coupon_code'}}</strong>\r\n                </div>\r\n                {{> components/cart/coupon-input}}\r\n            </div>\r\n        </li>\r\n    {{/if}}\r\n    {{#if cart.gift_wrapping_cost.amount}}\r\n        <li class=\"cart-total\">\r\n            <div class=\"wp-content clearfix\">\r\n                <div class=\"cart-total-label\">\r\n                    <strong>{{lang 'cart.checkout.gift_wrapping'}}</strong>\r\n                </div>\r\n                <div class=\"cart-total-value\">\r\n                    <span>{{cart.gift_wrapping_cost.formatted}}</span>\r\n                </div>\r\n            </div>\r\n        </li>\r\n    {{/if}}\r\n    <li class=\"cart-total\">\r\n         <div class=\"wp-content sub-total clearfix\">\r\n            <div class=\"cart-total-label\">\r\n                <strong>{{lang 'cart.checkout.subtotal'}}</strong>\r\n            </div>\r\n            <div class=\"cart-total-value\">\r\n                {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\r\n                    <span>{{cart.sub_total.formatted}}</span>\r\n                {{else}}\r\n                    {{> components/common/login-for-pricing}}\r\n                {{/or}}\r\n            </div>\r\n        </div>\r\n        {{#each cart.taxes}}\r\n                <div class=\"wp-content clearfix tax-cart\">\r\n                    <div class=\"cart-total-label\">\r\n                        <strong>{{name}}</strong>\r\n                    </div>\r\n                    <div class=\"cart-total-value\">\r\n                        <span>{{cost.formatted}}</span>\r\n                    </div>\r\n                </div>\r\n        {{/each}}\r\n    </li>\r\n\r\n\r\n    {{#if settings.gift_certificates_enabled}}\r\n        {{#each cart.gift_certificates}}\r\n            <li class=\"cart-total\">\r\n                 <div class=\"wp-content clearfix\">\r\n                    <div class=\"cart-total-label\">\r\n                        <strong>{{lang 'cart.gift_certificates.code_label' code=code}}</strong>\r\n                        <span>\r\n                            {{remaining.formatted}} | <a href=\"{{remove_url}}\">{{lang 'cart.gift_certificates.remove'}}</a>\r\n                        </span>\r\n                    </div>\r\n                    <div class=\"cart-total-value\">\r\n                        {{used.formatted}}\r\n                    </div>\r\n                </div>\r\n            </li>\r\n        {{/each}}\r\n        <li class=\"cart-total\">\r\n            <div class=\"cart-total-label\">\r\n                <strong>{{lang 'cart.gift_certificates.gift_certificate'}}:</strong>\r\n            </div>\r\n            {{> components/cart/gift-certificate-input}}\r\n        </li>\r\n    {{/if}}\r\n    <li class=\"cart-total\">\r\n         <div class=\"wp-content clearfix\">\r\n            <div class=\"cart-total-label\">\r\n                <strong>{{lang 'cart.checkout.grand_total'}}</strong>\r\n            </div>\r\n            <div class=\"cart-total-value cart-total-grandTotal\">\r\n                {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\r\n                    <span>{{cart.grand_total.formatted}}</span>\r\n                {{else}}\r\n                    {{> components/common/login-for-pricing}}\r\n                {{/or}}\r\n            </div>\r\n        </div>\r\n    </li>\r\n</ul>\r\n","components/common/login-for-pricing":"<p translate>Log in for pricing</p>"}
